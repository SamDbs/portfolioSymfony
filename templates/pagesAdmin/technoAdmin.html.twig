{% extends '/componentsAdmin/baseAdmin.html.twig' %}

{% block body %}
<div class="col-9">


 <table class="table">
        <tbody>
            <thead>
                <th></th>
                <th></th>
                <th>Nom de la techno</th>
                <th>Image</th>




            </thead>
                {% for techno in technos %}
